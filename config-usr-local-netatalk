afpd_c=/usr/local/netatalk/etc/afpd.conf
afpd_d=/usr/local/netatalk/sbin/afpd
afpdldap_c=/usr/local/netatalk/etc/afp_ldap.conf
applevolumedefault_c=/usr/local/netatalk/etc/AppleVolumes.default
applevolumesystem_c=/usr/local/etc/netatalk/AppleVolumes.system
atalk_c=/usr/local/etc/netatalk/atalkd.conf
atalkd_d=/usr/local/netatalk/sbin/atalkd
netatalk_c=/usr/local/netatalk/etc/netatalk.conf
netatalk_init=/etc/init.d/netatalk
pam_c=/etc/pam.d/netatalk
select_maxUsers=20
stop_netatalk=systemctl stop afpd atalkd papd timelord a2boot
start_netatalk=systemctl start afpd atalkd papd timelord a2boot
restart_netatalk=systemctl restart afpd atalkd papd timelord a2boot
