afpd_c=/opt/netatalk/etc/afpd.conf
afpd_d=/opt/netatalk/sbin/afpd
afpdldap_c=/opt/netatalk/etc/afp_ldap.conf
applevolumedefault_c=/opt/netatalk/etc/AppleVolumes.default
applevolumesystem_c=/opt/netatalk/etc/AppleVolumes.system
atalk_c=/opt/netatalk/etc/atalkd.conf
atalkd_d=/opt/netatalk/sbin/atalkd
netatalk_c=/opt/netatalk/etc/netatalk.conf
netatalk_init=/etc/init.d/netatalk
pam_c=/etc/pam.d/netatalk
papd_c=/opt/netatalk/etc/papd.conf
papd_d=/opt/netatalk/sbin/papd
stop_netatalk=systemctl stop afpd atalkd papd timelord a2boot
start_netatalk=systemctl start afpd atalkd papd timelord a2boot
restart_netatalk=systemctl restart afpd atalkd papd timelord a2boot
