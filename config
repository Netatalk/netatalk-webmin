afpd_c=/etc/netatalk/afpd.conf
afpd_d=/usr/sbin/afpd
afpdldap_c=/etc/netatalk/afp_ldap.conf
applevolumedefault_c=/etc/netatalk/AppleVolumes.default
applevolumesystem_c=/etc/netatalk/AppleVolumes.system
atalk_c=/etc/netatalk/atalkd.conf
atalkd_d=/usr/sbin/atalkd
netatalk_c=/etc/netatalk/netatalk.conf
netatalk_init=/etc/init.d/netatalk
pam_c=/etc/pam.d/netatalk
papd_c=/etc/netatalk/papd.conf
papd_d=/usr/sbin/papd
stop_netatalk=systemctl stop afpd atalkd papd timelord a2boot
start_netatalk=systemctl start afpd atalkd papd timelord a2boot
restart_netatalk=systemctl restart afpd atalkd papd timelord a2boot
