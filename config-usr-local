afpd_c=/usr/local/etc/afpd.conf
afpd_d=/usr/local/sbin/afpd
afpdldap_c=/usr/local/etc/afp_ldap.conf
applevolumedefault_c=/usr/local/etc/AppleVolumes.default
applevolumesystem_c=/usr/local/etc/AppleVolumes.system
atalk_c=/usr/local/etc/atalkd.conf
atalkd_d=/usr/local/sbin/atalkd
netatalk_c=/usr/local/etc/netatalk.conf
netatalk_init=/etc/init.d/netatalk
pam_c=/etc/pam.d/netatalk
papd_c=/usr/local/etc/papd.conf
papd_d=/usr/local/sbin/papd
stop_netatalk=systemctl stop afpd atalkd papd timelord a2boot
start_netatalk=systemctl start afpd atalkd papd timelord a2boot
restart_netatalk=systemctl restart afpd atalkd papd timelord a2boot
